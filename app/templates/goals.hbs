<div class='main-content'>
  <div class='layout-row layout-align-center-center'>
    {{outlet}}
  </div>
  <div class='layout-row layout-align-center-center'>
    {{#paper-content class="md-whiteframe-z1 list-demo"}}
      {{#paper-list}}
        {{#each model as |goal|}}
          {{#paper-item class='md-3-line goal'}}
            <div class='md-list-item-text'>
              <h3>{{goal.description}}</h3>
            </div>
          {{/paper-item}}
        {{/each}}
      {{/paper-list}}
    {{/paper-content}}
  </div>
</div>
